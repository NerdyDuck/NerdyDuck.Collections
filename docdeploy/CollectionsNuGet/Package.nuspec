<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<metadata minClientVersion="3.2">
		<id>NerdyDuck.Collections</id>
		<version>1.0.1</version>
		<title>NerdyDuck Collections</title>
		<authors>Daniel Kopp</authors>
		<description>This library provides a set of specialized collections, e.g. for multi-threading purposes.</description>
		<releaseNotes>
			v1.0.1: Fixed: Wrong build action for Resources.tt breaks NuGet package for UWP.
			v1.0.0: First release
		</releaseNotes>
		<summary>Specialized collections for .NET 4.6+ and UWP.</summary>
		<language>en-US</language>
		<projectUrl>https://github.com/NerdyDuck/NerdyDuck.Collections</projectUrl>
		<iconUrl>http://static.nerdyduck.de/NuGet/NerdyDuck.Collections-50x50.png</iconUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
		<copyright>Copyright Daniel Kopp 2015-2016</copyright>
		<dependencies>
			<dependency id="NerdyDuck.CodedExceptions" version="1.1.2"/>
		</dependencies>
		<references></references>
		<tags>collections lists concurrent synchronization uwp desktop</tags>
	</metadata>
	<files>
		<!-- Dummy file to mark package usable for UAP also in VS package manager (actual targeting for UAP must be done using the ref and runtimes folders) -->
		<file src="..\..\src\CollectionsUniversal\bin\Release\NerdyDuck.Collections.xml" target="lib\uap10.0\" />

		<!-- Simple Txm for .Net 4.5 desktop version-->
		<file src="..\..\src\CollectionsDesktop\bin\Release\NerdyDuck.Collections.dll" target="lib\net46\" />
		<file src="..\..\src\CollectionsDesktop\bin\Release\NerdyDuck.Collections.pdb" target="lib\net46\" />
		<file src="..\..\src\CollectionsDesktop\bin\Release\NerdyDuck.Collections.xml" target="lib\net46\" />
		<file src="..\..\src\CollectionsDesktop\bin\Release\de-DE\NerdyDuck.Collections.resources.dll" target="lib\net46\de-DE\" />

		<!-- Reference files for UAP design-time -->
		<file src="..\..\src\CollectionsUniversal\bin\Release\NerdyDuck.Collections.dll" target="ref\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\Release\NerdyDuck.Collections.xml" target="ref\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\Release\NerdyDuck.Collections.pri" target="ref\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\Collections.rd.xml" target="ref\uap10.0\NerdyDuck.Collections\" />

		<!-- Platform-specific files for UAP compile-time and run-time -->
		<file src="..\..\src\CollectionsUniversal\Collections.rd.xml" target="runtimes\win10-arm\lib\uap10.0\NerdyDuck.Collections\" />
		<file src="..\..\src\CollectionsUniversal\bin\ARM\Release\NerdyDuck.Collections.dll" target="runtimes\win10-arm\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\ARM\Release\NerdyDuck.Collections.pdb" target="runtimes\win10-arm\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\ARM\Release\NerdyDuck.Collections.pri" target="runtimes\win10-arm\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\Collections.rd.xml" target="runtimes\win10-x86\lib\uap10.0\NerdyDuck.Collections\" />
		<file src="..\..\src\CollectionsUniversal\bin\x86\Release\NerdyDuck.Collections.dll" target="runtimes\win10-x86\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\x86\Release\NerdyDuck.Collections.pdb" target="runtimes\win10-x86\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\x86\Release\NerdyDuck.Collections.pri" target="runtimes\win10-x86\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\Collections.rd.xml" target="runtimes\win10-x64\lib\uap10.0\NerdyDuck.Collections\" />
		<file src="..\..\src\CollectionsUniversal\bin\x64\Release\NerdyDuck.Collections.dll" target="runtimes\win10-x64\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\x64\Release\NerdyDuck.Collections.pdb" target="runtimes\win10-x64\lib\uap10.0\" />
		<file src="..\..\src\CollectionsUniversal\bin\x64\Release\NerdyDuck.Collections.pri" target="runtimes\win10-x64\lib\uap10.0\" />

		<!-- Source files for the symbols package -->
		<file src="..\..\src\CollectionsDesktop\**\*.cs" exclude="..\..\src\CollectionsDesktop\obj\**\*.cs" target="src\CollectionsDesktop\" />
		<file src="..\..\src\CollectionsUniversal\**\*.cs" exclude="..\..\src\CollectionsUniversal\obj\**\*.cs" target="src\CollectionsUniversal\" />
	</files>
</package>
