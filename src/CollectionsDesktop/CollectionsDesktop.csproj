  <ItemGroup>
    <ResWFiles Include="..\CollectionsUniversal\Strings\en-US\Resources.resw">
      <Link>Properties\en-US\Resources.resw</Link>
      <ResourceName>NerdyDuck.Collections.Properties.Resources</ResourceName>
    </ResWFiles>
    <ResWFiles Include="..\CollectionsUniversal\Strings\de-DE\Resources.resw">
      <Link>Properties\de-DE\Resources.resw</Link>
      <ResourceName>NerdyDuck.Collections.Properties.Resources.de-DE</ResourceName>
    </ResWFiles>
  </ItemGroup>
  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <Target Name="BeforeBuild">
    <!-- Need to copy/rename the *.resw files to *.resx, because GenerateResource does not recognize the .resw file extension. Copied files are renamed to the full name of the resource. -->
    <Copy SourceFiles="@(ResWFiles)" DestinationFiles="@(ResWFiles->'obj\$(ConfigurationName)\%(ResourceName).resx')" SkipUnchangedFiles="true" OverwriteReadOnlyFiles="true">
      <Output TaskParameter="CopiedFiles" ItemName="ResXFiles" />
    </Copy>
    <!-- Generate resource files -->
    <GenerateResource Sources="@(ResXFiles)" OutputResources="@(ResXFiles->'obj\$(ConfigurationName)\%(Filename).resources')">
      <Output TaskParameter="OutputResources" ItemName="GeneratedResources" />
    </GenerateResource>
    <!-- Adding generated resource files to project as embedded resource.
    Because the file names include the culture (e.g. NerdyDuck.ParameterValidation.Properties.Resources.de-DE.resources),
    the appropriate satellite assemblies are generated automatically. The resource file without a culture (that is en-US)
    is embedded into the main assembly. -->
    <ItemGroup>
      <EmbeddedResource Include="obj\$(ConfigurationName)\%(ResWFiles.ResourceName).resources">
        <LogicalName>%(ResWFiles.ResourceName).resources</LogicalName>
      </EmbeddedResource>
    </ItemGroup>
  </Target>
  <Target Name="AfterBuild">
    <ItemGroup>
      <!-- Getting output assemblies and generated satellite assemblies -->
      <FilesToSign Include="$(TargetDir)**\$(TargetName)*$(TargetExt)" />
      <FilesToSign Condition="'$(TargetExt)' != '.dll'" Include="$(TargetDir)**\$(TargetName)**.dll" />
    </ItemGroup>
    <PropertyGroup>
      <!-- Get installation path of Windows SDK 7.1A, because it contains signtool.exe -->
      <WinSDK71Path>$(registry:HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Microsoft SDKs\Windows\v7.1A@InstallationFolder)</WinSDK71Path>
      <SignHashAlg>sha256</SignHashAlg>
      <TimestampServer>http://tsa.startssl.com/rfc3161</TimestampServer>
    </PropertyGroup>
    <!-- Sign all assemblies, using the default SPC from the user store -->
    <Exec Condition=" '$(Configuration)' == 'Release' " Command="&quot;$(WinSDK71Path)bin\signtool.exe&quot; sign /a /q /fd $(SignHashAlg) /tr $(TimestampServer) &quot;%(FilesToSign.Identity)&quot;" />
  </Target>
</Project>